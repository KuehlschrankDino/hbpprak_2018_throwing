<bibi
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://schemas.humanbrainproject.eu/SP10/2014/BIBI" xsi:schemaLocation="http://schemas.humanbrainproject.eu/SP10/2014/BIBI ../bibi_configuration.xsd">
  <brainModel>
    <file>idle_brain.py</file>
    <populations population="sensors" from="0" step="1" to="768" xsi:type="Range" />
    <populations population="dopamine" from="768" step="1" to="770" xsi:type="Range" />
    <populations population="actors" from="770" step="1" to="786" xsi:type="Range" />
    <populations population="plot" from="0" step="1" to="786" xsi:type="Range" />
    <populations population="population_0" from="0" step="1" to="1" xsi:type="Range" />
  </brainModel>
  <bodyModel assetPath="arm_robot" customAsset="false">arm_robot_gripper.sdf</bodyModel>
  
  <transferFunction xsi:type="PythonTransferFunction" src="simple_move_robot.py" />
  <transferFunction xsi:type="PythonTransferFunction" src="grab_image.py" />
</bibi>
